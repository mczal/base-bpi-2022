      /! begin::User
      .topbar-item
        .btn.btn-icon.btn-icon-mobile.w-auto.btn-clean.d-flex.align-items-center.btn-lg.px-2[
          data-controller="base--metronics--quick-user-toggle"
          data-action="click->base--metronics--quick-user-toggle#handleClick"
        ]
          span.text-muted.font-weight-bold.font-size-base.d-none.d-md-inline.mr-1
            | Hi,
          span.text-dark-50.font-weight-bolder.font-size-base.d-none.d-md-inline.mr-3
            |  #{current_user.name}
          span.symbol.symbol-lg-35.symbol-25.symbol-light-success
            span.symbol-label.font-size-h5.font-weight-bold
              | #{current_user.name.first(1).upcase}

        / #kt_quick_user_toggle.btn.btn-icon.btn-icon-mobile.w-auto.btn-clean.d-flex.align-items-center.btn-lg.px-2
