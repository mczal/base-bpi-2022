      /! begin::Header Menu
      #kt_header_menu.header-menu.header-menu-mobile.header-menu-layout-default
        /! begin::Header Nav
        ul.menu-nav
          li.menu-item 
            / = image_tag "logo.png", class: "logo-default mr-2", size: "120x20"
            a href="/"
              span.text-dark.font-weight-boldest.font-size-h4.font-size-lg-h2 = current_company.name.titleize
        /! end::Header Nav
      /! end::Header Menu
    /! end::Header Menu Wrapper
